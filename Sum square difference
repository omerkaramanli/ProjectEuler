using System;
using System.Collections.Generic;

namespace euler
{
    class Program
    {
        static void Main(string[] args){
            int sumOfSquares = 0;
            int squareOfSums = 0;
            for(int i = 0; i <= 100; i++){ 
                sumOfSquares += i*i;
                squareOfSums += i;
            }
            squareOfSums *= squareOfSums;
            int result = squareOfSums - sumOfSquares;

            Console.WriteLine(result);
        }
    }
}
