using System;
					
public class Program
{
	public static void Main()
	{
		int n1 = 1, n2 = 2, result = 0;
        int fibonacci =0;
        result += n2;
        while(fibonacci <= 4000000){
            fibonacci = n1+n2;
            n1 = n2;
            n2 = fibonacci;
            if(fibonacci%2 == 0){
                result += fibonacci;
            }
        }
        Console.Write(result);
        Console.Write("sup");
	}
}
