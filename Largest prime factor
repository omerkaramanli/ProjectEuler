using System;

namespace euler
{
    class Program
    {
        static void Main(string[] args)
        {
            //long prime = 0;
            long num = 600851475143;
            long i = 0;
            for(i = 2; i<= num; i++){
                if(num%i == 0){
                    num /= i;
                    i--;
                }
            }
            Console.WriteLine(i);
        }
    }
}
